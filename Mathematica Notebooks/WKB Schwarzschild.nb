Cell[BoxData[
 RowBox[{
  RowBox[{"DerivPotential", "[", 
   RowBox[{"xx_", ",", "nn_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Initial", " ", 
      RowBox[{"value", ":", " ", 
       RowBox[{"0", "th", " ", "derivative"}]}]}], ",", " ", 
     RowBox[{
      RowBox[{"i", ".", "e", ".", " ", "\[IndentingNewLine]", "the"}], " ", 
      "potential", " ", "itself"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"result", "=", "xx"}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"For", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", "1"}], ",", 
      RowBox[{"(*", 
       RowBox[{"Counter", " ", "starts", " ", "at", " ", "1"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"n", "\[LessEqual]", " ", "nn"}], ",", 
      RowBox[{"(*", 
       RowBox[{"Repeat", " ", "until", " ", "nn", " ", 
        RowBox[{"(", 
         RowBox[{"=", " ", 
          RowBox[{"number", " ", "of", " ", "derivatives"}]}], ")"}]}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"n", "++"}], ",", " ", 
      RowBox[{"(*", "Step", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"result", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"2", "/", "r"}]}], ")"}], 
        RowBox[{"D", "[", 
         RowBox[{"result", ",", "r"}], "]"}]}]}]}], 
     RowBox[{"(*", 
      RowBox[{
      "Use", " ", "previous", " ", "result", " ", "to", " ", "compute", " ", 
       "the", " ", "next"}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", "result"}], 
   "\[IndentingNewLine]", ")"}]}]], "Input",
